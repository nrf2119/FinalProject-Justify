<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nigel.finalproject_justify">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogInActivity" />
        <activity
            android:name=".InquiryActivity"

            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".MakeArgumentActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".SeeMyOwnActivity"></activity>
        <activity
            android:name=".SeeOthersActivity"
            android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".PositionActivity"
            android:parentActivityName=".InquiryActivity" />
        <activity android:name=".ReadArgumentActivity"></activity>
    </application>

</manifest>
